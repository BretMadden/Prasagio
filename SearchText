function get_occurrence(varS,string){ c=(string.split(varS).length - 1); return c; } ; console. log("Total Occurrence is "+get_occurrence("peace",temp2.wholeText)); String.prototype.timesCharExist = function ( chr ) { var total = 0, last_location = 0, single_char = ( chr + '' ) [0]; while( last_location = this.indexOf( single_char, last_location ) + 1 ) { total = total + 1; } return total; }; 
